@model List<Audit>;


<table class="table">
    <thead>
        <tr>
            <th>
                <span>&#8470;</span>
            </th>
            <th>
                UserId
            </th>
            <th>
                Type
            </th>
            <th>
                ChangedDate
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @item.Id
                </td>
                <td>
                    @item.UserId
                </td>
                <td>
                    @item.Type
                </td>
                <td>
                    @item.DateTime.ToString("dd.MM.yyyy HH:mm")
                </td>
            </tr>
        }
    </tbody>
</table>
